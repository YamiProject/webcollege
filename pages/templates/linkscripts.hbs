<link rel="shortcut icon" href="{{#base64 "/img/icons/favicon.png"}}{{/base64}}" type="image/png"/>
<link rel="stylesheet" type="text/css" href="{{#base64 "/css/bootstrap.min.css"}}{{/base64}}"/>
<link rel="stylesheet" type="text/css" href="{{#base64 "/css/bootstrap-grid.min.css"}}{{/base64}}"/>
<link rel="stylesheet" type="text/css" href="{{#base64 "/css/main.css"}}{{/base64}}"/>
{{#equal options.theme_id 1}}
    <link rel="stylesheet" type="text/css" href="{{#base64 "/css/themes/theme_1.css"}}{{/base64}}"/>
{{/equal}}
{{#equal options.theme_id 2}}
    <link rel="stylesheet" type="text/css" href="{{#base64 "/css/themes/theme_2.css"}}{{/base64}}"/>
{{/equal}}
{{#equal options.theme_id 3}}
    <link rel="stylesheet" type="text/css" href="{{#base64 "/css/themes/theme_3.css"}}{{/base64}}"/>
{{/equal}}
{{#equal options.theme_id 4}}
    <link rel="stylesheet" type="text/css" href="{{#base64 "/css/themes/theme_4.css"}}{{/base64}}"/>
{{/equal}}
{{#undefine options.theme_id}}
    {{else}}
    <link rel="stylesheet" type="text/css" href="{{#base64 "/css/themes/theme_1.css"}}{{/base64}}"/>
{{/undefine}}
<script src="{{#base64 "/js/jQuery.js"}}{{/base64}}"></script>
<script src="{{#base64 "/js/bootstrap.min.js"}}{{/base64}}"></script>
<script src="{{#base64 "/js/bootstrap.bundle.min.js"}}{{/base64}}"></script>
<script src="/js/main.js" type="module"></script>
{{#equal role 'teacher'}}
    <script src="/js/teach.js" type="module"></script>
{{/equal}}
{{#equal role 'head'}}
    <script src="/js/head.js" type="module"></script>
{{/equal}}
{{#equal role 'student'}}
    <script src="/js/stud.js" type="module"></script>
{{/equal}}
<script src="/js/socket.io.js"></script>