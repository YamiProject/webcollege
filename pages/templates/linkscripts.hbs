<link rel="shortcut icon" href="/img/icons/favicon.png" type="image/png"/>
<link rel="stylesheet" type="text/css" href="/css/bootstrap.min.css"/>
<link rel="stylesheet" type="text/css" href="/css/bootstrap-grid.min.css"/>
<link rel="stylesheet" type="text/css" href="/css/main.css"/>
{{#equal options.theme_id 1}}
    <link rel="stylesheet" type="text/css" href="/css/themes/theme_1.css"/>
{{/equal}}
{{#equal options.theme_id 2}}
    <link rel="stylesheet" type="text/css" href="/css/themes/theme_2.css"/>
{{/equal}}
{{#equal options.theme_id 3}}
    <link rel="stylesheet" type="text/css" href="/css/themes/theme_3.css"/>
{{/equal}}
{{#undefine options.theme_id}}
    {{else}}
    <link rel="stylesheet" type="text/css" href="/css/themes/theme_1.css"/>
{{/undefine}}
<script src="/js/jQuery.js"></script>
<script src="/js/bootstrap.min.js"></script>
<script src="/js/bootstrap.bundle.min.js"></script>
<script src="/js/main.js" type="module"></script>
{{#equal role 'teacher'}}
    <script src="/js/teach.js" type="module"></script>
{{/equal}}
{{#equal role 'head'}}
    <script src="/js/head.js" type="module"></script>
{{/equal}}
{{#equal role 'student'}}
    <script src="/js/stud.js" type="module"></script>
{{/equal}}